{
    "version": "1.0.0",
    "description": "Registry of all available workflow nodes. Use this as a reference for generating Workflow JSON objects.",
    "node_schema_reference": {
        "id": "unique-node-id",
        "kind": "trigger | action | logic",
        "ui": {
            "label": "User facing label",
            "position": {
                "x": 0,
                "y": 0
            }
        },
        "spec": {
            "node_name": "the.exact.node.type",
            "config": {
                "param": "value"
            },
            "runtime": {
                "type": "internal"
            }
        }
    },
    "available_nodes": [
        {
            "category": "Trigger",
            "name": "manual.trigger",
            "description": "Manually triggers a workflow from the UI or API.",
            "snippet": {
                "kind": "trigger",
                "spec": {
                    "node_name": "manual.trigger",
                    "config": {}
                }
            }
        },
        {
            "category": "Trigger",
            "name": "schedule.cron",
            "description": "Triggers workflow on a schedule.",
            "snippet": {
                "kind": "trigger",
                "spec": {
                    "node_name": "schedule.cron",
                    "config": {
                        "frequency": "minutes",
                        "interval": 15
                    }
                }
            }
        },
        {
            "category": "Trigger",
            "name": "webhook.receive",
            "description": "Triggers when a POST request is sent to the webhook URL.",
            "snippet": {
                "kind": "trigger",
                "spec": {
                    "node_name": "webhook.receive",
                    "config": {
                        "path": "/my-hook"
                    }
                }
            }
        },
        {
            "category": "Trigger",
            "name": "email.receive",
            "description": "Triggers when an email is received.",
            "snippet": {
                "kind": "trigger",
                "spec": {
                    "node_name": "email.receive",
                    "config": {
                        "address": "inbox@example.com"
                    }
                }
            }
        },
        {
            "category": "Trigger",
            "name": "form.submit",
            "description": "Triggers when a linked form is submitted.",
            "snippet": {
                "kind": "trigger",
                "spec": {
                    "node_name": "form.submit",
                    "config": {
                        "form_id": "contact-us"
                    }
                }
            }
        },
        {
            "category": "Trigger",
            "name": "rss.read",
            "description": "Triggers when a new item is found in an RSS feed.",
            "snippet": {
                "kind": "trigger",
                "spec": {
                    "node_name": "rss.read",
                    "config": {
                        "url": "https://rss.feed/url"
                    }
                }
            }
        },
        {
            "category": "Trigger",
            "name": "whatsapp.receive",
            "description": "Triggers on incoming WhatsApp message.",
            "snippet": {
                "kind": "trigger",
                "spec": {
                    "node_name": "whatsapp.receive",
                    "config": {
                        "phone_number_id": "YOUR_PHONE_ID"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "http.request",
            "description": "Makes an HTTP request.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "http.request",
                    "config": {
                        "method": "GET",
                        "url": "https://api.example.com",
                        "headers": {
                            "Authorization": "Bearer token"
                        }
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "code.python",
            "description": "Executes Python code in a sandbox.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "code.python",
                    "runtime": {
                        "type": "code",
                        "language": "python"
                    },
                    "config": {
                        "code": "return {'message': 'Hello ' + input_data.get('name')}"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "code.javascript",
            "description": "Executes JavaScript code in a sandbox.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "code.javascript",
                    "runtime": {
                        "type": "code",
                        "language": "javascript"
                    },
                    "config": {
                        "code": "return { message: `Hello ${input.name}` };"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "data.transform",
            "description": "Transforms input data using simplified mapping logic.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "data.transform",
                    "config": {
                        "mapping": {
                            "new_field": "{{input.old_field}}"
                        }
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "data.set",
            "description": "Sets a static or dynamic variable in the workflow context.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "data.set",
                    "config": {
                        "key": "variable_name",
                        "value": "variable_value"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "data.store",
            "description": "Persists data to long-term storage.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "data.store",
                    "config": {
                        "key": "db_key",
                        "value": "{{input.data}}"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "ai.llm",
            "description": "Generates text using an LLM.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "ai.llm",
                    "config": {
                        "model": "google/gemini-2.0-flash-exp:free",
                        "prompt": "Summarize this: {{input.text}}",
                        "credential": "YOUR_CRED_ID"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "ai.agent",
            "description": "Runs an autonomous AI agent with a goal.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "ai.agent",
                    "config": {
                        "goal": "Research topic X",
                        "model": "gpt-4",
                        "credential": "YOUR_CRED_ID"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "slack.send",
            "description": "Sends a message to Slack.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "slack.send",
                    "config": {
                        "webhook_url": "YOUR_WEBHOOK_OR_CRED",
                        "channel": "#general",
                        "text": "Hello world"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "discord.send",
            "description": "Sends a message to Discord.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "discord.send",
                    "config": {
                        "webhook_url": "YOUR_WEBHOOK",
                        "content": "Hello Discord"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "email.send",
            "description": "Sends an email.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "email.send",
                    "config": {
                        "to": "recipient@example.com",
                        "subject": "Notification",
                        "body": "Content here"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "whatsapp.send",
            "description": "Sends a WhatsApp message.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "whatsapp.send",
                    "config": {
                        "to": "PHONE_NUMBER",
                        "text": "Hello from automation"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "google_sheets.read",
            "description": "Reads data from Google Sheets.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "google_sheets.read",
                    "config": {
                        "spreadsheet_id": "SHEET_ID",
                        "range": "Sheet1!A1:B10",
                        "auth": "YOUR_CRED_ID"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "google_sheets.write",
            "description": "Writes data to Google Sheets.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "google_sheets.write",
                    "config": {
                        "spreadsheet_id": "SHEET_ID",
                        "range": "Sheet1!A1",
                        "data": [
                            [
                                "Col1",
                                "Col2"
                            ]
                        ],
                        "auth": "YOUR_CRED_ID"
                    }
                }
            }
        },
        {
            "category": "Action",
            "name": "utility.noop",
            "description": "No Operation. Passes data through unchanged.",
            "snippet": {
                "kind": "action",
                "spec": {
                    "node_name": "utility.noop",
                    "config": {}
                }
            }
        },
        {
            "category": "Logic",
            "name": "condition.if",
            "description": "Standard If/Else condition.",
            "snippet": {
                "kind": "logic",
                "spec": {
                    "node_name": "condition.if",
                    "config": {
                        "condition": "{{input.value}} > 10"
                    }
                }
            }
        },
        {
            "category": "Logic",
            "name": "condition.switch",
            "description": "Switch/Case logic for multiple branches.",
            "snippet": {
                "kind": "logic",
                "spec": {
                    "node_name": "condition.switch",
                    "config": {
                        "value": "{{input.status}}",
                        "cases": {
                            "active": "output_1",
                            "inactive": "output_2"
                        }
                    }
                }
            }
        },
        {
            "category": "Logic",
            "name": "logic.loop",
            "description": "Iterates over a list of items.",
            "snippet": {
                "kind": "logic",
                "spec": {
                    "node_name": "logic.loop",
                    "config": {
                        "items": "{{input.list_data}}"
                    }
                }
            }
        },
        {
            "category": "Logic",
            "name": "logic.merge",
            "description": "Merges multiple execution branches back into one.",
            "snippet": {
                "kind": "logic",
                "spec": {
                    "node_name": "logic.merge",
                    "config": {
                        "wait_for": "all"
                    }
                }
            }
        },
        {
            "category": "Logic",
            "name": "logic.delay",
            "description": "Pauses execution for a set time.",
            "snippet": {
                "kind": "logic",
                "spec": {
                    "node_name": "logic.delay",
                    "config": {
                        "seconds": 5
                    }
                }
            }
        },
        {
            "category": "Logic",
            "name": "logic.parallel",
            "description": "Starts multiple branches in parallel.",
            "snippet": {
                "kind": "logic",
                "spec": {
                    "node_name": "logic.parallel",
                    "config": {
                        "branches": 2
                    }
                }
            }
        },
        {
            "category": "Logic",
            "name": "execution.pause",
            "description": "Pauses execution until manual resume.",
            "snippet": {
                "kind": "logic",
                "spec": {
                    "node_name": "execution.pause",
                    "config": {
                        "reason": "Awaiting approval"
                    }
                }
            }
        }
    ]
}